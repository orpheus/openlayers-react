{
  "name": "ol6-react",
  "version": "0.1.4",
  "description": "react wrapper for openlayers-v6",
  "files": ["/dist"],
  "main": "dist/ol6-react-bundle.cjs.min.js",
  "module": "dist/ol6-react-bundle.esm.min.js",
  "browser": "dist/ol6-react-bundle.umd.min.js",
  "scripts": {
    "transpile": "npx babel src --out-dir lib",
    "transpile-watch": "npx babel src --watch --out-dir lib",
    "bundle": "npx rollup -c",
    "build": "npm run transpile && npm run bundle",
    "lint": "eslint --ext js,jsx --fix src/",
    "prepublish": "npm run build"
  },
  "author": "orpheus (ohryan)",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "hex-to-rgba": "^2.0.1",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "^4.0.1",
    "ol": "^6.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.18.1",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-postcss": "^3.1.4"
  }
}
